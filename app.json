{
  "expo": {
    "name": "GrocersApp",
    "slug": "GrocersApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "mdpi": "./assets/img/splash/splash-mdpi.png",
        "hdpi": "./assets/img/splash/splash-hdpi.png",
        "xhdpi": "./assets/img/splash/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash/splash-xxxhdpi.png",
        "resizeMode": "contain"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
